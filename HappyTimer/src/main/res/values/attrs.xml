<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircularCountDownTimer">
        <attr name="timer_total_seconds" format="integer"/>
        <attr name="timer_time_elapsed_interval" format="integer"/>
        <attr name="stroke_foreground_color" format="color" />
        <attr name="stroke_background_color" format="color" />
        <attr name="stroke_foreground_thickness" format="dimension" />
        <attr name="stroke_background_thickness" format="dimension" />
        <attr name="timer_text_size" format="dimension" />
        <attr name="timer_text_color" format="color" />
        <attr name="timer_text_isBold" format="boolean" />
    </declare-styleable>

</resources>